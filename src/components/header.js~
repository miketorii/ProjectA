import React from 'react';

class Header extends React.Component {

  constructor(props) {
    super(props);

    this.state = {
      hval001: 1234,
      hstr001: 'Header123'
    }; 
  }

  const changevalue = () => {
    setState({val777: 567});

    var msg = 'Tom Header';
    setState({str777: msg});
  };

  render() {
    return (
      <div className='App-headerfooter'>
          <h3>hval = {this.state.hval001}, hstr = {this.state.hstr001}
          <button onClick={changevalue}>Change</button></h3>
      </div>
    );
  }

}

export default Header;
